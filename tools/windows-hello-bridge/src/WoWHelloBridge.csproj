<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0-windows10.0.19041.0</TargetFramework>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <SelfContained>true</SelfContained>
    <PublishSingleFile>true</PublishSingleFile>
    <EnableCompressionInSingleFile>true</EnableCompressionInSingleFile>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>WoWHelloBridge</AssemblyName>
    <Version>1.0.0</Version>
    <Authors>Chude</Authors>
    <Description>Windows Hello bridge for WoW SuperAdmin authentication</Description>
  </PropertyGroup>

  <!-- Windows Runtime APIs are included automatically with net9.0-windows targeting -->

</Project>
